{
  "name": "birthdayy-discord-bot",
  "version": "0.2.3",
  "main": "dist/index.js",
  "author": "@chillihero",
  "dependencies": {
    "@devtomio/plugin-botlist": "^1.3.0",
    "@kaname-png/plugin-subcommands-advanced": "^2.2.1",
    "@prisma/client": "^4.14.1",
    "@sapphire/cron": "^1.1.0",
    "@sapphire/decorators": "^6.0.1",
    "@sapphire/discord.js-utilities": "^6.1.0",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.4.3",
    "@sapphire/plugin-api": "^5.0.4",
    "@sapphire/plugin-editable-commands": "^3.0.2",
    "@sapphire/plugin-hmr": "^2.0.1",
    "@sapphire/plugin-i18next": "^5.0.3",
    "@sapphire/plugin-logger": "^3.0.4",
    "@sapphire/plugin-scheduled-tasks": "^7.0.0",
    "@sapphire/plugin-subcommands": "^4.0.1",
    "@sapphire/plugin-utilities-store": "^1.0.1",
    "@sapphire/result": "^2.6.4",
    "@sapphire/stopwatch": "^1.5.0",
    "@sapphire/time-utilities": "^1.7.9",
    "@sapphire/type": "^2.4.0",
    "@sapphire/utilities": "^3.11.2",
    "@sentry/integrations": "^7.52.1",
    "@sentry/node": "^7.52.1",
    "@sentry/tracing": "^7.52.1",
    "@skyra/env-utilities": "^1.2.0",
    "bullmq": "^3.13.4",
    "colorette": "^2.0.20",
    "dayjs": "^1.11.7",
    "discord-api-types": "^0.37.42",
    "discord.js": "^14.11.0",
    "prisma": "^4.14.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@sapphire/cli": "^1.6.1",
    "@sapphire/eslint-config": "^4.4.2",
    "@sapphire/ts-config": "^4.0.0",
    "@types/node": "^20.2.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "docker compose -f ./docker/docker-compose.yml up -d",
    "down": "docker compose -f ./docker/docker-compose.yml down",
    "build": "docker compose -f ./docker/docker-compose.yml build",
    "logs": "docker compose -f ./docker/docker-compose.yml logs -f",
    "shell": "docker exec -it birthdayy_bot bash",
    "doppler": "DOPPLER_TOKEN=\"$(doppler configs tokens create dev --plain --max-age 1m)\"",
    "dev": "yarn run doppler \\docker compose -f ./docker/docker-compose.dev.yml up",
    "dev:legacy": "doppler run -- yarn node --enable-source-maps dist/index.js",
    "dev:watch": "tsc-watch",
    "dev:build": "docker compose -f ./docker/docker-compose.dev.yml build",
    "dev:down": "docker compose -f ./docker/docker-compose.dev.yml down",
    "dev:shell": "docker exec -it birthdayy-dev bash",
    "prepare": "husky install",
    "lint:fix": "eslint --fix --ext .ts,.js ./src",
    "clean": "node scripts/clean.mjs",
    "lint": "eslint --ext .ts,.js ./src",
    "dev:migrate": "docker exec -it birthdayy-dev npx prisma migrate dev",
    "dev:generate": "docker exec -it birthdayy-dev npx prisma generate",
    "db:migrate": "docker exec -it birthdayy npx prisma migrate deploy",
    "db:generate": "docker exec -it birthdayy npx prisma generate"
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BirthdayyBot/BirthdayyBotSapphire.git"
  }
}
